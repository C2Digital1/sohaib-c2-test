  .featured-collection-section .featured-collection .flickity-slider {
    display: flex; 
  }
  .featured-collection-section .featured-collection .flickity-slider .product__thumbnail {
    display: flex; height: 100%; 
  } 
  */
  /* Common Styles Like Buttons Color padding etc*/
  .featured-collection-section a.button {
    min-height: 54px; display: flex; align-items: center; 
    border-radius: 8px; min-width: 160px; margin-top: 30px; 
  }
  .blog-card__read-more a:hover, .blog-card__read-more a:focus {
    color: {{settings.link_hover_color}} !important; opacity: 1; 
  }
  .blog-card__read-more a.button {
    text-align: left; background: unset !important; opacity: 1;  font-size: 80%; 
    text-decoration: unset !important; border-bottom: unset !important; font-weight: bold; 
    justify-content: flex-start; width: auto !important; min-height: unset; 
  }
  .blog-card__content .excerpt {
    max-height: 4.7em; line-height: 1.5em; margin-bottom: 0px; 
  }
  .blog-card__content .title.is-text {
    margin-bottom: 1rem;  font-size: 18px;
  }
  .blog-card__content .truncation-fade , .blog-card__content .meta-info {
    display: none; 
  }
  /* Media Queries For Mobile */

