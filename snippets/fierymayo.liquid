  .navbar-item.has-mega-menu .mega-menu {
    max-width: 70%; margin-left: 15%; min-width: 70%; 
    padding: 20px;
  }
  .mega-menu .menu__heading {
    padding-bottom: 10px; border-bottom: 1px solid #fff; max-width: 94%; margin-left: 0px; 
    padding-left: 0px;  margin-bottom: 20px; font-size: 18px; 
  }

  
    /*/////////////// */ 
   /*  Footer Blocks Css */
  /*/////////////// */ 
  .footer__menu p.footer__heading , .footer__block.block__newsletter  span[data-icon="email"] , 
  .d-none {
    display: none; 
  }
  .footer__menu a {
    font-size: 16px; line-height: 25px; 
    font-family: {{'Exo Regular ' | append: fonts }};
  }
  .footer__block.block__newsletter div.field , .footer__block.block__newsletter div.field div.control   {
     width: 100%; max-width: 100%; 
  }

  
  /*/////////////////////////////////////////////////////////// */ 
  /*   Footer Block Newsletter Styles */
  /*/////////////////////////////////////////////////////////// */ 

  button.in-email span.icon {
    max-width: 80%; height: auto; margin: 0px auto; 
  }
  button.in-email {
    height: 1.5em; width: 1.8em; padding: 0px; top: 26%;; 
    position: absolute; right: 0.5em; border-radius: 4px;
  }
  button.in-email:hover svg , button.in-email:focus svg {
    fill: #00badb; 
  }
  
  .footer__block.block__newsletter div.field input[type="email"] {
    height: 64px; border-radius: 8px; padding-left: 0.5em;
  }
  .footer__block.block__newsletter div.field input[type="email"]::-moz-placeholder {
      font-size: 14px; 
  }
  .footer__block.block__newsletter div.field input[type="email"]::placeholder {
      font-size: 14px; 
  }

  /*/////////////////////////////////////////////////////////// */ 
  /*   Buttons Has no border if its background is transparent */
  /*/////////////////////////////////////////////////////////// */ 
  
  .button:not(.in-email):not(div) {
    border-style: solid; min-height: 54px; display: flex; align-items:center; 
  }
  .button:hover , .button:focus {
    border-style: solid; 
  }
  
  /*/////////////////////////////////////////////////////////// */ 
  /*   Feature Collection Slider Css For Dots and arrows */
  /*/////////////////////////////////////////////////////////// */ 
  .featured-collection-section  .flickity-page-dots {
    display: flex; align-items: center; 
  }
 
  .featured-collection-section .flickity-button:disabled {
    display: none; 
  }
  
 .product__thumbnail .product-wrap {
    box-shadow: 0 2px 5px 0 rgba(0,0,0,0.1); 
    padding-bottom: 10px; margin: 5px 0px; 
  }
 .product__thumbnail .product-thumbnail__title {
    font-size: 18px; width: 100%; 
    min-height: 47px; font-weight: bold; 
  }
  .has-thumbnail-sticker .product-thumbnail__price {
    display: none; 
  }
  .product__thumbnail .product__imageContainer {
    position: relative;padding-bottom: 85%; overflow: hidden;
  }
  .product__thumbnail .image-element__wrap {
    background-color: #d9d9d9;  max-height: unset !important; 
     max-width: 100% !important; width: 100% !important; 
  }
  .product-thumbnail {
    padding: 5px 8px; padding-top: 10px; 
  }
  .arrow-link {
    text-align: right; margin-top: 10px; 
  }
  .product__thumbnail .image-element__wrap img {
    position: absolute; width: auto !important; max-height: 100% !important; 
    height: 100% !important; left: 0px; top: 0px; right: 0;bottom: 0;; 
    margin: 0px auto
  }
  .featured-collection-section flickity-button:disabled{
    display: none; 
  }
  .featured-collection-section .flickity-button {
    top: 45%; 
  }
  .featured-collection-section .flickity-page-dots {
    right: 5px; bottom: 10px; 
  }
